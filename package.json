{
  "name": "agency-website",
  "version": "1.0.0",
  "description": "Building a fast, lightweight website using Eleventy and SCSS",
  "main": ".eleventy.js",
  "scripts": {
    "purgecss": "purgecss --config purgecss.config.js --output dist/styles",
    "build": "npx eleventy && npm run purgecss",
    "dev": "npx eleventy --serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dipshanadh/agency-website.git"
  },
  "keywords": [
    "eleventy",
    "scss"
  ],
  "author": "Dipshan Adhikari <dipshanadh@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/dipshanadh/agency-website/issues"
  },
  "homepage": "https://github.com/dipshanadh/agency-website#readme",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-fetch": "^4.0.0",
    "@11ty/eleventy-img": "^3.1.1",
    "gorko": "^0.9.1",
    "html-minifier": "^4.0.0",
    "lightningcss": "^1.22.0",
    "purgecss": "^5.0.0",
    "sass": "^1.69.4"
  }
}
